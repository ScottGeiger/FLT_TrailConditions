{
  "name": "flt_trailconditions",
  "version": "1.0.0",
  "description": "React implementation of the FLT Trail Conditions",
  "main": "index.html",
  "scripts": {
    "build": "npm-run-all -s clean-js webpack -s list",
    "build-local": "npm-run-all -s clean-js webpack-local -s list",
    "build-dev": "npm-run-all -s clean-js webpack-dev list",
    "start-dev": "webpack-dev-server --mode=development",
    "webpack": "webpack --mode production --config ./webpack.config.js",
    "webpack-local": "webpack --env local --mode production --config ./webpack.config.js",
    "webpack-dev": "webpack --mode development --config ./webpack.config.js",
    "build-css": "cleancss src/css/styles.css -o public/css/styles.min.css",
    "build-html": "html-minifier -c etc/html-minifier.conf src/index.html -o public/index.html",
    "clean-js": "rm -f public/js/*",
    "clean-dist": "find public/ -type f -not -path \"*/api/*\" -delete",
    "list": "find public/ -type f -not -path \"*/api/*\" -printf \"%Tb %Td %TH:%TM\\t%k\\t%p\\n\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScottGeiger/FLT_TrailConditions.git"
  },
  "author": "Scott Geiger",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ScottGeiger/FLT_TrailConditions/issues"
  },
  "homepage": "https://github.com/ScottGeiger/FLT_TrailConditions#readme",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.3",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.8.1",
    "@iconify/react": "^3.2.2",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "@tanstack/query-sync-storage-persister": "^4.0.10",
    "@tanstack/react-query": "^4.1.3",
    "@tanstack/react-query-devtools": "^4.0.10",
    "@tanstack/react-query-persist-client": "^4.0.10",
    "bootstrap": "^5.2.0",
    "date-fns": "^2.28.0",
    "html-minifier": "^4.0.0",
    "htmr": "^1.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.2.1",
    "react-scroll": "^1.8.7"
  }
}
