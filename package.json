{
  "name": "flt_trailconditions",
  "version": "1.0.0",
  "description": "React implementation of the FLT Trail Conditions",
  "main": "index.html",
  "scripts": {
    "build": "npm-run-all -s clean-dist -p build-html webpack -s list",
    "build-dev": "npm-run-all -s clean-js webpack-dev list",
    "start-dev": "webpack-dev-server --mode=development",
    "webpack": "webpack --config ./webpack.config.js",
    "webpack-dev": "webpack --mode development --config ./webpack.config.js",
    "build-css": "cleancss src/css/styles.css -o public/css/styles.min.css",
    "build-html": "html-minifier -c conf/html-minifier.conf src/index.html -o public/index.html",
    "clean-js": "rm -f public/js/*",
    "clean-dist": "find public/ -type f -not -path \"*/api/*\" -delete",
    "list": "find public/ -type f -not -path \"*/api/*\" -printf \"%Tb %Td %TH:%TM\\t%k\\t%p\\n\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScottGeiger/FLT_TrailConditions.git"
  },
  "author": "Scott Geiger",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/ScottGeiger/FLT_TrailConditions/issues"
  },
  "homepage": "https://github.com/ScottGeiger/FLT_TrailConditions#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.4.4",
    "npm-run-all": "^4.1.5",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.4.1",
    "date-fns": "^2.28.0",
    "htmr": "^1.0.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.14",
    "react-router-dom": "^6.2.1"
  }
}
